<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.VueStatistiques">
   <children>
      <BorderPane prefHeight="600.0" prefWidth="800.0">
         <top>
            <Label text="Label" BorderPane.alignment="CENTER" />
         </top>
         <bottom>
            <Label text="Label" BorderPane.alignment="CENTER" />
         </bottom>
         <right>
            <Button mnemonicParsing="false" onAction="#Retour" text="Button" BorderPane.alignment="CENTER" />
         </right>
         <left>
            <ComboBox fx:id="comboBox" onAction="#SwapDeck" prefWidth="150.0" BorderPane.alignment="CENTER" />
         </left>
         <center>
            <ScrollPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <content>
                  <VBox prefHeight="970.0" prefWidth="591.0">
                     <children>
                        <PieChart fx:id="PieChart1" />
                        <PieChart fx:id="PieChart2" />
                        <PieChart fx:id="PieChart3" />
                     </children>
                  </VBox>
               </content>
            </ScrollPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
